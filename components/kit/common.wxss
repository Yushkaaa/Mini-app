/* Main styles */

/* Overriding predefined styles */
button::after {
	border-radius: 0;
  content: ' ';
  border: none;
}
button::before {
	border-radius: 0;
  content: ' ';
  border: none;
}
button {
  border: none;
  border-radius: 0px;
  font-size: 15px;
  color: white;
  text-align: center;
  text-decoration: none;
}
/* BTN */
Btn.floating {
  position: fixed;
  width: 100%;
  display: block;
  bottom: 0;
  left: 0;
}
/* WRAPPERS */
.element-wrapper-15 {
  display: block;
  width: calc(100% - 30px);
  padding: 0 15px;
  height: auto;
  position: relative;
}
.cut-by-1 {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.cut-by-2 {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
