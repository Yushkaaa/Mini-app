.font {
  font-family: Verdana, Geneva, Tahoma, sans-serif;
}
.back-button {
  position: absolute;
  top: 0;
  left: 0;
  width: 30px;
  height: 30px;
  display: block;
  z-index: 1;
}
.backBtn-title-wrapper {
  position: relative;
  width: 100%;
}
.backBtn-title-wrapper Title {
